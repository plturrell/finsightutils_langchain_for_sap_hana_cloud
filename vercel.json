{
  "version": 2,
  "name": "langchain-hana-app",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python@3.1.0",
      "config": {
        "runtime": "python3.10",
        "maxDuration": 60
      }
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/docs",
      "dest": "api/index.py"
    },
    {
      "src": "/api/openapi.json",
      "dest": "api/index.py"
    },
    {
      "src": "/api/texts",
      "methods": ["POST"],
      "dest": "api/index.py"
    },
    {
      "src": "/api/query",
      "methods": ["POST"],
      "dest": "api/index.py"
    },
    {
      "src": "/api/query/vector",
      "methods": ["POST"],
      "dest": "api/index.py"
    },
    {
      "src": "/api/delete",
      "methods": ["POST"],
      "dest": "api/index.py"
    },
    {
      "src": "/api/benchmark/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/api/health",
      "dest": "api/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "API_HOST": "0.0.0.0",
    "API_PORT": "8000",
    "LOG_LEVEL": "INFO",
    "GPU_ENABLED": "false",
    "USE_INTERNAL_EMBEDDINGS": "false",
    "EMBEDDING_MODEL": "all-MiniLM-L6-v2",
    "REACT_APP_API_URL": "/api"
  }
}