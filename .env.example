# SAP HANA Cloud Connection
HANA_HOST=your-hana-host.hanacloud.ondemand.com
HANA_PORT=443
HANA_USER=your_username
HANA_PASSWORD=your_password
DEFAULT_TABLE_NAME=GPU_ACCELERATED_VECTORS

# API Configuration
PORT=8000
LOG_LEVEL=INFO
ENABLE_CORS=true
CORS_ORIGINS=*
JWT_SECRET=your-secret-key
DB_MAX_CONNECTIONS=5
DB_CONNECTION_TIMEOUT=600
      
# GPU Acceleration
GPU_ENABLED=true
USE_TENSORRT=true
TENSORRT_PRECISION=fp16
TENSORRT_ENGINE_CACHE_DIR=/app/trt_engines
BATCH_SIZE=32
MAX_BATCH_SIZE=128
ENABLE_MULTI_GPU=true
GPU_MEMORY_FRACTION=0.9
      
# Error Handling
ENABLE_CONTEXT_AWARE_ERRORS=true
ERROR_VERBOSITY=standard
ENABLE_ERROR_TELEMETRY=true
      
# Vector Operations
ENABLE_PRECISE_SIMILARITY=true
DEFAULT_EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
ENABLE_VECTOR_VISUALIZATION=true

# Blue-Green Deployment (for docker-compose.blue-green.yml)
BLUE_VERSION=1.0.0
GREEN_VERSION=1.0.0
CHECK_INTERVAL=30
SWITCH_THRESHOLD=3
GRAFANA_ADMIN_PASSWORD=admin