# SAP HANA Cloud Connection
HANA_HOST=your-hana-host.hanacloud.ondemand.com
HANA_PORT=443
HANA_USER=SYSTEM
HANA_PASSWORD=your-password-here
DEFAULT_TABLE_NAME=EMBEDDINGS

# API Configuration
PORT=8000
LOG_LEVEL=INFO
ENABLE_CORS=true
CORS_ORIGINS=*
TEST_MODE=false

# GPU Acceleration
GPU_ENABLED=true
USE_TENSORRT=true
TENSORRT_PRECISION=fp16
BATCH_SIZE=32
MAX_BATCH_SIZE=128
ENABLE_MULTI_GPU=true
GPU_MEMORY_FRACTION=0.9

# Vector Operations
DEFAULT_EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2