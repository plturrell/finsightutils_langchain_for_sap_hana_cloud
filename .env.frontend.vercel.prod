# Production environment configuration for Vercel frontend

# API URL (will be overridden during deployment)
NEXT_PUBLIC_API_URL=${BACKEND_URL}

# Application configuration
NEXT_PUBLIC_APP_NAME=SAP HANA Cloud LangChain Integration
NEXT_PUBLIC_APP_DESCRIPTION=Integrate LangChain with SAP HANA Cloud Vector Engine
NEXT_PUBLIC_ENVIRONMENT=production

# UI Configuration
NEXT_PUBLIC_PRIMARY_COLOR=#0F62FE
NEXT_PUBLIC_SECONDARY_COLOR=#0072C3
NEXT_PUBLIC_ACCENT_COLOR=#D9FBFB

# Features
NEXT_PUBLIC_ENABLE_VECTOR_SEARCH=true
NEXT_PUBLIC_ENABLE_GRAPH_QUERY=true
NEXT_PUBLIC_ENABLE_CHAT=true
NEXT_PUBLIC_ENABLE_FILE_UPLOAD=true

# Analytics
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# Authentication (if needed)
# NEXT_PUBLIC_AUTH_PROVIDER=none