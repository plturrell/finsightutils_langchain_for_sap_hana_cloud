# Production environment configuration for Together.ai backend

# Platform configuration
BACKEND_PLATFORM=together_ai
ENVIRONMENT=prod
LOG_LEVEL=INFO
APP_NAME=langchain-hana-integration

# API configuration
ENABLE_CORS=true
CORS_ORIGINS=https://langchain-hana-frontend.vercel.app,https://www.yourdomain.com
REQUEST_TIMEOUT=60

# Together.ai configuration
TOGETHER_API_KEY=${TOGETHER_API_KEY}
TOGETHER_MODEL_NAME=togethercomputer/llama-2-7b
TOGETHER_API_BASE=https://api.together.xyz/v1
USE_TOGETHER_EMBEDDINGS=true

# SAP HANA Cloud configuration
HANA_HOST=${HANA_HOST}
HANA_PORT=${HANA_PORT}
HANA_USER=${HANA_USER}
HANA_PASSWORD=${HANA_PASSWORD}
HANA_ENCRYPT=true
HANA_SSL_VALIDATE=true
HANA_AUTOCOMMIT=true
HANA_MAX_CONNECTIONS=10
HANA_CONNECT_TIMEOUT=30
HANA_CONNECTION_POOLING=true

# Embedding configuration
EMBEDDING_MODEL_NAME=togethercomputer/m2-bert-80M-8k-retrieval
USE_INTERNAL_EMBEDDING=false
INTERNAL_EMBEDDING_MODEL_ID=SAP_NEB.20240715
CACHE_EMBEDDINGS=true
EMBEDDING_CACHE_DIR=/tmp/embedding_cache

# Vectorstore configuration
VECTORSTORE_TABLE_NAME=EMBEDDINGS
VECTORSTORE_CONTENT_COLUMN=VEC_TEXT
VECTORSTORE_METADATA_COLUMN=VEC_META
VECTORSTORE_VECTOR_COLUMN=VEC_VECTOR
VECTORSTORE_VECTOR_COLUMN_TYPE=REAL_VECTOR
VECTORSTORE_CREATE_HNSW_INDEX=true
VECTORSTORE_DISTANCE_STRATEGY=COSINE

# GPU configuration (disabled for Together.ai deployment)
GPU_ENABLED=false