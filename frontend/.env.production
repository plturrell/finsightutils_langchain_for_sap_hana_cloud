# Production environment configuration for Vercel frontend

# API URL - This will be set during the build process on Vercel
REACT_APP_API_URL=${BACKEND_URL}

# Application configuration
REACT_APP_APP_NAME=SAP HANA Cloud LangChain Integration
REACT_APP_APP_DESCRIPTION=Integrate LangChain with SAP HANA Cloud Vector Engine
REACT_APP_ENVIRONMENT=production
REACT_APP_API_VERSION=v1

# UI Configuration
REACT_APP_PRIMARY_COLOR=#0F62FE
REACT_APP_SECONDARY_COLOR=#0072C3
REACT_APP_ACCENT_COLOR=#D9FBFB

# Features
REACT_APP_ENABLE_VECTOR_SEARCH=true
REACT_APP_ENABLE_GRAPH_QUERY=true
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_FILE_UPLOAD=true

# Analytics
REACT_APP_ENABLE_ANALYTICS=true

# Connection Configuration
REACT_APP_CONNECTION_TIMEOUT=60000
REACT_APP_RETRY_COUNT=3
REACT_APP_RETRY_DELAY=2000

# Authentication (enabled for production)
REACT_APP_AUTH_REQUIRED=true