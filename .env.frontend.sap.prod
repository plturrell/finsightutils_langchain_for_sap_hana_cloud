# SAP BTP Frontend Production Environment Configuration

# API Configuration - Will be replaced by deploy script based on backend
REACT_APP_API_URL=

# Environment Configuration
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=1.2.0

# Feature Flags
REACT_APP_SHOW_DEBUG=false
REACT_APP_MOCK_API=false
REACT_APP_ENABLE_VISUALIZATION=true
REACT_APP_ENABLE_KNOWLEDGE_GRAPH=true
REACT_APP_ENABLE_ADVANCED_FILTERING=true

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_MAX_SEARCH_RESULTS=20
REACT_APP_ENABLE_ANIMATIONS=true

# Analytics
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ANALYTICS_ID=

# Authentication
REACT_APP_AUTH_ENABLED=true
REACT_APP_AUTH_PROVIDER=sap