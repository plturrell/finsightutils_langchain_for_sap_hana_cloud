# Backend Environment Configuration Template
# This file serves as a template for different deployment environments

# Platform Configuration
PLATFORM=vercel
PLATFORM_SUPPORTS_GPU=false
ENVIRONMENT=development

# Server Configuration
API_HOST=0.0.0.0
API_PORT=8000
LOG_LEVEL=INFO
DEBUG=false

# Database Configuration
HANA_HOST=
HANA_PORT=443
HANA_USER=
HANA_PASSWORD=
HANA_ENCRYPT=true
HANA_SSL_VALIDATE_CERT=true

# Vector Store Configuration
DEFAULT_TABLE_NAME=EMBEDDINGS
DEFAULT_CONTENT_COLUMN=VEC_TEXT
DEFAULT_METADATA_COLUMN=VEC_META
DEFAULT_VECTOR_COLUMN=VEC_VECTOR
VECTOR_COLUMN_TYPE=REAL_VECTOR
VECTOR_COLUMN_LENGTH=-1

# GPU Configuration
GPU_ENABLED=false
GPU_DEVICE=auto
GPU_BATCH_SIZE=32
EMBEDDING_MODEL=all-MiniLM-L6-v2
USE_INTERNAL_EMBEDDINGS=false
INTERNAL_EMBEDDING_MODEL_ID=SAP_NEB.20240715

# TensorRT Configuration
USE_TENSORRT=false
TENSORRT_PRECISION=fp16
TENSORRT_CACHE_DIR=/tmp/tensorrt_engines
TENSORRT_DYNAMIC_SHAPES=true

# Together.ai Configuration
TOGETHER_API_KEY=
TOGETHER_MODEL=togethercomputer/m2-bert-80M-8k-retrieval

# Feature Flags
ENABLE_ERROR_CONTEXT=true
ENABLE_PRECISE_SIMILARITY=true
ENABLE_KNOWLEDGE_GRAPH=true
ENABLE_ADVANCED_CLUSTERING=false
ENABLE_RATE_LIMITING=true
ENABLE_MOCK_MODE=false
ENABLE_CORS=true